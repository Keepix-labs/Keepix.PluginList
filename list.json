      "icon": "https://raw.githubusercontent.com/Keepix-labs/Keepix.tokens/main/icons/XTZ.png",
      "platforms": ["linux-arm64", "linux-x64", "win-x64", "win-arm64", "oxs-x64", "oxs-arm64"],
      "requires": {
        "ports": [],
         "installForm": {
           "inputs": [
              {
                  "key": "WalletSecretKey",
                  "type": "wallet",
